{
    "build": {
        "beforeBuildCommand": "npm run build",
        "beforeDevCommand": "npm run dev",
        "devPath": "http://localhost:5173",
        "distDir": "../dist"
    },
    "package": {
        "productName": "roseate"
    },
    "tauri": {
        "allowlist": {
            "all": false,
            "window": {
                "all": true
            },
            "os": {
                "all": false
            },
            "protocol": {
                "all": false,
                "asset": true,
                "assetScope": ["**"]
            }
        },
        "cli": {
            "description": "ðŸŒ¹ A fancy image viewer.",
            "longDescription": "ðŸŒ¹ A small and simple but fancy image viewer built with Rust & Typescript that's cross platform.",
            "beforeHelp": "",
            "afterHelp": "",
            "args": [
                {
                    "name": "theme",
                    "takesValue": true,
                    "multiple": false,
                    "possibleValues": ["dark", "light"]
                },
                {
                    "index": 1,
                    "name": "source",
                    "takesValue": true
                }
            ]
        },
        "bundle": {
            "active": true,
            "category": "DeveloperTool",
            "copyright": "",
            "deb": {
                "depends": []
            },
            "externalBin": [],
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ],
            "identifier": "xyz.devgoldy.cloudy.roseate",
            "longDescription": "A small and simple but fancy image viewer built with Rust & Typescript that's cross platform.",
            "macOS": {
                "entitlements": null,
                "exceptionDomain": "",
                "frameworks": [],
                "providerShortName": null,
                "signingIdentity": null
            },
            "resources": [],
            "shortDescription": "",
            "targets": "all",
            "windows": {
                "certificateThumbprint": null,
                "digestAlgorithm": "sha256",
                "timestampUrl": ""
            }
        },
        "security": {
            "csp": "default-src 'self'; img-src 'self' asset: https://asset.localhost"
        },
        "updater": {
            "active": false
        },
        "windows": [
            {
                "visible": false,
                "fullscreen": false,
                "height": 600,
                "resizable": true,
                "title": "Roseate",
                "width": 800
            }
        ]
    }
}